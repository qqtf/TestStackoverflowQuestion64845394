@model IEnumerable<ClassLibrary1.Entities.A_State>

@{
    ViewData["Title"] = "A_StateList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="main-body">
    <div class="page-wrapper">
        <div class="page-header card">
            <div class="card-block">
                <h5 class="m-b-10">Editable Table</h5>
                <p class="text-muted m-b-10"></p>
                <ul class="breadcrumb-title b-t-default p-t-10">
                    <li class="breadcrumb-item">
                        <a href="index.html"> <i class="fa fa-home"></i> </a>
                    </li>
                    <li class="breadcrumb-item">
                        <a href="#!">Table</a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="page-body">
            <div class="card">
                <div class="card-header">
                    <h5></h5>
                    <span></span>
                </div>
                <div class="card-block">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered" id="example-2">
                            <thead>
                                <tr>
                                    <th>State ID</th>
                                    <th>State Name</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <th scope="row">1</th>
                                        <td class="tabledit-view-mode">
                                            <span class="tabledit-span">@item.State_ID</span>
                                            <input class="tabledit-input form-control input-sm" type="text" name="First" value="Senem">
                                        </td>
                                        <td class="tabledit-view-mode">
                                            <span class="tabledit-span">@item.State_Name</span>
                                            <input class="tabledit-input form-control input-sm" type="text" name="Last" value="Can">
                                        </td>
                                        <td class="tabledit-view-mode">
                                            <span class="tabledit-span"></span>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
